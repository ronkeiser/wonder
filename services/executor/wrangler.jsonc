{
  "name": "wonder-executor",
  "main": "src/index.ts",
  "compatibility_date": "2025-12-01",

  "ai": {
    "binding": "AI"
  },

  "services": [
    { "binding": "LOGS", "service": "wonder-logs" },
    { "binding": "EVENTS", "service": "wonder-events" }
  ],

  "durable_objects": {
    "bindings": [
      {
        "name": "COORDINATOR",
        "class_name": "WorkflowCoordinator",
        "script_name": "wonder-coordinator"
      }
    ]
  },

  "observability": {
    "enabled": true
  }
}
